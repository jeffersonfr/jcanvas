prefix="@CMAKE_INSTALL_PREFIX@"
exec_prefix="${prefix}"
libdir="${prefix}/lib"
includedir="${prefix}/include"

Name: jCanvas
Description: A library for abstract a host graphic engine
URL: https://github.com/me/jeffersonfr/jcanvas
Version: 0.0.1
Requires: cairo freetype2 jmixin
Requires.private: 
Cflags: -I"${includedir}" @JCANVAS_INCLUDE_DIRS@
Cflags.private:
Libs: -L"${libdir}" -l$<TARGET_FILE_BASE_NAME:jCanvas::Algebra> -l$<TARGET_FILE_BASE_NAME:jCanvas::Core> $<$<TARGET_EXISTS:jCanvas::Widgets>:-l$<TARGET_FILE_BASE_NAME:jCanvas::Widgets>>
Libs.private: 
